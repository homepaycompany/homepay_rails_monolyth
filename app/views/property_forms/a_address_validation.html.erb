<div class="main-section">
  <div class="row">
    <div class="col-xs-12 col-sm-8 col-sm-offset-2">
      <div class="form-container">
        <div class="form-title text-center">
          <h1>Addresse du bien</h1>
        </div>
        <div class="form-body">
          <div id="map" style="width: 100%;height: 300px; margin-bottom: 20px" data-markers="<%= @marker.to_json %>"></div>
        </div>
        <div class="form-validation">
          <div class="btn form-button" id="go-back">
            Retour
          </div>
          <div class="btn form-button" id="validate-section">
            Valider
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%= link_to property_form_property_type_path(@property_form), method: :get do %>
  <div class="btn hidden" id="move-to-next-section"></div>
<% end %>
